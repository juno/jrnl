<div class="autopagerize_page_element">
  <% @posts.each do |post| %>
    <article class="post">
      <header>
        <h1><%= link_to post.created_at, post %></h1>
      </header>
      <div class="content">
        <%= raw post.html %>
      </div>
    </article>
  <% end %>
</div>

<%= will_paginate @posts %>
